<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="m1sirsoirandroid.triviaquiz">

    <application
        android:allowBackup="true"
        android:icon="@drawable/triviafinal"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".HomeScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PlayAgain" />
        <activity android:name=".GameWon" />
        <activity android:name=".Time_Up" />
        <activity android:name=".HighScore"/>
        <activity android:name=".HighScores"/>
        <activity android:name=".MainGameActivity">

        </activity>
    </application>

</manifest>